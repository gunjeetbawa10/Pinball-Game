<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wingcompilerfun.pinballgame"
    android:installLocation="auto"
    android:versionCode="22"
    android:versionName="1.0.1" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="26" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:testOnly="true"
        android:theme="@style/Theme.VPTheme" >
        <activity
            android:name="com.wingcompilerfun.pinballgame.BouncyActivity"
            android:configChanges="keyboard|keyboardHidden|orientation"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.wingcompilerfun.pinballgame.AboutActivity" >
        </activity>
        <activity android:name="com.wingcompilerfun.pinballgame.BouncyPreferences" >
        </activity>
    </application>

</manifest>